        "InstanceIAMRole": {
            "Properties": {
                "AssumeRolePolicyDocument": {
                    "Statement": [
                        {
                            "Action": [
                                "sts:AssumeRole"
                            ],
                            "Effect": "Allow",
                            "Principal": {
                                "Service": [
                                    "ec2.amazonaws.com"
                                ]
                            }
                        }
                    ],
                    "Version": "2012-10-17"
                },
                "Path": "/",
                "Policies": [
                    {
                        "PolicyDocument": {
                            "Statement": [
                                {
                                    "Action": [
                                        "autoscaling:DetachInstances",
                                        "autoscaling:DescribeAutoScalingInstances",
                                        "autoscaling:DescribeAutoScalingGroups"
                                    ],
                                    "Effect": "Allow",
                                    "Resource": "*"
                                },
