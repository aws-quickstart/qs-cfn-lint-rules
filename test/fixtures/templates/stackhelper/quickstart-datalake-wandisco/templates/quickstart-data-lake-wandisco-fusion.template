            "Properties": {
                "AssociatePublicIpAddress": "true",
                "BlockDeviceMappings": {
                    "Fn::If": [
                        "EphemeralStorage",
                        [
                            {
                                "DeviceName": "/dev/sdb",
                                "VirtualName": "ephemeral0"
                            },
                            {
                                "DeviceName": "/dev/sdc",
                                "VirtualName": "ephemeral1"
                            },
                            {
                                "DeviceName": "/dev/sdd",
                                "VirtualName": "ephemeral2"
                            },
                            {
                                "DeviceName": "/dev/sde",
                                "VirtualName": "ephemeral3"
                            }
                        ],
                        [
                            {
                                "DeviceName": "/dev/sdb",
                                "Ebs": {
                                    "DeleteOnTermination": "True",
                                    "VolumeSize": {
                                        "Ref": "PersistentStorage"
                                    }
                                }
                            },
                            {
                                "DeviceName": "/dev/sdc",
                                "Ebs": {
                                    "DeleteOnTermination": "True",
                                    "VolumeSize": {
                                        "Ref": "PersistentStorage"
                                    }
                                }
                            },
                            {
                                "DeviceName": "/dev/sdd",
                                "Ebs": {
                                    "DeleteOnTermination": "True",
                                    "VolumeSize": {
                                        "Ref": "PersistentStorage"
                                    }
                                }
                            },
                            {
                                "DeviceName": "/dev/sde",
                                "Ebs": {
                                    "DeleteOnTermination": "True",
                                    "VolumeSize": {
