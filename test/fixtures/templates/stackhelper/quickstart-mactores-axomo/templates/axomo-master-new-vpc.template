            "DependsOn":"VPCStack",
            "Type":"AWS::CloudFormation::Stack",
            "Properties":{
                "TemplateURL":{
                    "Fn::Sub":"https://${QSS3BucketName}.s3.amazonaws.com/${QSS3KeyPrefix}templates/axomo-workload.template"
                },
                "Parameters":{
                    "QSS3BucketName":{
                        "Ref":"QSS3BucketName"
                    },
                    "QSS3KeyPrefix":{
                        "Ref":"QSS3KeyPrefix"
                    },
                    "VPCID":{
                        "Fn::GetAtt":[
                            "VPCStack",
                            "Outputs.VPCID"
