                    "CreateDMSRoles":{
                        "Ref":"CreateDMSRoles"
                    },
                    "AthenaDBname":{
                        "Ref":"AthenaDBname"
                    },
                    "CrawlerDBname":{
                        "Ref":"CrawlerDBname"
                    },
                    "SagemakerEndpointConfigInstanceType":{
                        "Ref":"SagemakerEndpointConfigInstanceType"
                    },
                    "SagemakerModelName":{
