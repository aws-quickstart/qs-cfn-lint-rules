      ActionsEnabled: 'true'
      AlarmActions:
        - !Ref 'BigipScaleDownPolicy'
      AlarmDescription: Throughput below average threshold for 10 successive intervals
        of 5 minutes
      ComparisonOperator: LessThanThreshold
      EvaluationPeriods: '10'
      MetricName: throughput-per-sec
