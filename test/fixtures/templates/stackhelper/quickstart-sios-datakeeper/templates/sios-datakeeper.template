            - SDKCEWIN2016BYOL
          - !FindInMap
            - AWSAMIRegionMap
            - !Ref AWS::Region
            - SDKCEWIN2012R2BYOL
        - !If
          - OSVersionCondition
          - !FindInMap
            - AWSAMIRegionMap
            - !Ref AWS::Region
            - SDKCEWIN2016
          - !FindInMap
            - AWSAMIRegionMap
            - !Ref AWS::Region
