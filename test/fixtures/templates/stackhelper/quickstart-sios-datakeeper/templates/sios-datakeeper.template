            - !Ref DomainMemberSGID
            - !Ref WSFCSecurityGroup
            - !Ref WSFCClientSecurityGroup
            - !Ref SIOSDataKeeperSG
      Tags:
        - Key: Name
          Value: !Ref WSFCNode1NetBIOSName
      BlockDeviceMappings:
        - DeviceName: /dev/sda1
          Ebs:
            VolumeSize: 127
            VolumeType: gp2
        - DeviceName: /dev/xvdca
          VirtualName: ephemeral0
      KeyName: !Ref KeyPairName
