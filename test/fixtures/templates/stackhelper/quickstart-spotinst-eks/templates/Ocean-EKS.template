                "GPU": "AMZNEKS110GPU"
            }
        },
        "AMITypesMap": {
            "Regular": {
                "Type": "STD"
            }, 
            "GPU": {
                "Type": "GPU"
            }
        },
        "AWSAMIRegionMap": {
            "AMI": {
                "AMZNEKS111": "amazon-eks-node-1.11-v20", 
                "AMZNEKS111GPU": "amazon-eks-gpu-node-1.11-v20", 
                "AMZNEKS110": "amazon-eks-node-1.10-v20", 
                "AMZNEKS110GPU": "amazon-eks-gpu-node-1.10-v20"
            }, 
            "ap-northeast-1": {
